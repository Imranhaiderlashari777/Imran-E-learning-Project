{
    "compilerOptions": {
      "target": "ES2021",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "allowJs": true,
      "checkJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["./"],
    "exclude": ["node_modules", ".cache", "build"]
  }
  